  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.55.0",
    "@tailwindcss/postcss": "4.1.11",
    "@types/bun": "1.2.20",
    "@types/node": "24.2.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "prisma": "6.14.0",
